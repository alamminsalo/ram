package model

{{#if has_datetime}}
import (
  "time"
)
{{/if}}

{{#if is_object}}
type {{pascalcase name}} struct {
{{#each properties}}
  {{ pascalcase name }} {{ type }} `json:"{{ camelcase name }}" {{ ext "x-go-custom-tag" }}`
{{/each}}
{{#if additional_properties}}
{{#with additional_properties}}
{{#each properties}}
  {{ pascalcase name }} {{type}} `json:"-" {{ ext "x-go-custom-tag" }}`
{{/each}}
{{/with}}
{{/if}}
}
{{/if}}
{{#if is_array}}
type {{name}} {{type}}
{{/if}}
